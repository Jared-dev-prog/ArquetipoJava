type Query {
	[%= namelower %]s(
		query : [%= namecamel %]Query
		page : Int
		size : Int
	) : [[%= namecamel %]]
}

input [%= namecamel %]Query {
	id: Int
	city: String
	phone: String
	addressLine1: String
	addressLine2: String
	state: String
	country: String
	postalCode: String
	territory: String
}

type [%= namecamel %] {
	id: Int
	city: String
	phone: String
	addressLine1: String
	addressLine2: String
	state: String
	country: String
	postalCode: String
	territory: String
}
